{
  "name": "dynamic-wallpaper",
  "version": "0.0.2",
  "main": "desktop/main.js",
  "author": {
    "name": "DTJ"
  },
  "description": "dynamic wallpaper",
  "scripts": {
    "start": "vite --host",
    "vite:dev": "vite --open --host",
    "vite:build": "vue-tsc --noEmit && vite build",
    "vite:preview": "vite preview",
    "electron:start": "electron .",
    "electron:dev": "wait-on tcp:3000 && electron .",
    "electron": "concurrently -k \"npm start\" \"npm run electron:dev\"",
    "electron:build": "npm run vite:build && node desktop/foundation/build.js"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/node": "^17.0.45",
    "@vitejs/plugin-vue": "^2.3.3",
    "concurrently": "^7.2.2",
    "electron": "16.0.7",
    "electron-builder": "^22.14.13",
    "naive-ui": "^2.30.5",
    "typescript": "^4.7.4",
    "vite": "^2.9.12",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16",
    "vue-tsc": "^0.29.8",
    "wait-on": "^6.0.1",
    "zip-a-folder": "^1.1.3"
  },
  "dependencies": {
    "wallpaper": "^4.4.2"
  }
}
